<div class="meme">
  <%=image_tag(@meme.image_url, size: "400x400", class:"meme_show", alt:@meme.title)%>


<h1 class="meme_show"> <%=@meme.title%> </h1>
</div>
<hr>
<%= render "captions/captions" %>
<br>
<%= form_for :caption do |f|  %>
<%= f.text_field :title %>
<%= f.text_field :signature %>
<%= f.submit "submit" %>
<% end %>



