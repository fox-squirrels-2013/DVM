<header>
  <h1 class="header">PIMP MY MEME</h1>
</header>

<h2>add a meme</h2>

<% form_for :meme, url: memes_path(:meme) do |f| %>
<%= f.text_field :title %>

<%= f.submit "submit" %>
<% end %>

<div class = "memes">

<% @memes.shuffle.each do |meme|%>
  <%= link_to image_tag(meme.image_url, size: "200x200", class:"meme", alt:meme.title), meme_path(meme) %>
<%end %>

</div>
